{
    "name": "pces-mui",
    "license": "UNLICENSED",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "lint": "next lint",
        "extract:i18n": "formatjs extract \"{pages,components,containers}/**/*.{js,ts,tsx}\" --ignore=\"**/*.d.ts\" --out-file \"src/i18n/extracted/EN.json\" --id-interpolation-pattern '[sha512:contenthash:base64:6]' --format formatter.js",
        "compile:i18n": "formatjs compile-folder --ast \"src/i18n/extracted\" \"src/i18n/compiled\" --format formatter.js",
        "i18n": "npm run extract:i18n && npm run compile:i18n",
        "format": "prettier --write",
        "cy:open": "cypress open",
        "cy:run": "cypress run --component"
    },
    "dependencies": {
        "@date-io/date-fns": "^2.16.0",
        "@emotion/react": "^11.10.6",
        "@emotion/styled": "^11.10.6",
        "@faker-js/faker": "^7.6.0",
        "@mui/icons-material": "^5.11.11",
        "@mui/lab": "^5.0.0-alpha.124",
        "@mui/material": "^5.11.14",
        "@mui/x-date-pickers": "^6.0.3",
        "@tanstack/match-sorter-utils": "^8.8.4",
        "@tanstack/react-table": "^8.8.4",
        "@tanstack/table-core": "^8.8.4",
        "@types/node": "18.15.10",
        "@types/react": "18.0.29",
        "@types/react-dom": "18.0.11",
        "animate.css": "^4.1.1",
        "axios": "^1.3.4",
        "classnames": "^2.3.2",
        "date-fns": "^2.29.3",
        "eslint": "8.36.0",
        "eslint-config-next": "13.2.4",
        "formik": "^2.2.9",
        "moment": "^2.29.4",
        "mui-one-time-password-input": "^1.1.0",
        "next": "13.2.4",
        "next-auth": "^4.20.1",
        "react": "18.2.0",
        "react-auth-code-input": "^3.2.1",
        "react-countup": "^6.4.2",
        "react-dom": "18.2.0",
        "react-dropzone": "^14.2.3",
        "react-iframe": "^1.8.5",
        "react-intl": "^6.3.2",
        "react-phone-input-2": "^2.15.1",
        "react-query": "^3.39.3",
        "recharts": "^2.5.0",
        "typescript": "5.0.2",
        "yup": "^1.0.2"
    },
    "devDependencies": {
        "@formatjs/cli": "^6.0.4",
        "@next/eslint-plugin-next": "^13.2.4",
        "@types/recharts": "^1.8.24",
        "@typescript-eslint/eslint-plugin": "^5.56.0",
        "@typescript-eslint/parser": "^5.56.0",
        "babel-plugin-formatjs": "^10.4.0",
        "cypress": "^12.8.1",
        "eslint-config-prettier": "^8.8.0",
        "eslint-config-standard": "^17.0.0",
        "eslint-plugin-cypress": "^2.12.1",
        "eslint-plugin-import": "^2.27.5",
        "eslint-plugin-jsx-a11y": "^6.7.1",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-react": "^7.32.2",
        "husky": "^8.0.3",
        "lint-staged": "^13.2.0",
        "prettier": "^2.8.7"
    },
    "lint-staged": {
        "*.{js,jsx,ts,tsx,json}": [
            "eslint --fix"
        ],
        "*.{js,jsx,ts,tsx,json,css,scss,md}": [
            "prettier --write"
        ]
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    }
}